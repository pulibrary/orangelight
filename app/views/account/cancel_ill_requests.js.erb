import AlertManager from './alert_manager';
const data = {
  message: "<%= message %>",
  success: "<%= success %>",
};
const alertManager = new AlertManager();

if (data.success) {
  alertManager.displaySuccess(data);
} else {
  alertManager.displayErrors(data);
}

<% unless @account.nil? %>
  document.getElementById('ill-request-cancel').innerHTML = "<%= escape_javascript(render :partial=>'ill_request_items') %>";
<% end %>
