const data = {
  message: "<%= @message %>",
  success: "<%= @success %>",
};

if (data.success) {
  globalThis.alertManager.displaySuccess(data);
} else {
  data.errors = "<%= @message %>";
  globalThis.alertManager.displayErrors(null, data);
}

<% unless @account.nil? %>
  document.getElementById('ill-request-cancel').innerHTML = "<%= escape_javascript(render :partial=>'ill_request_items') %>";
<% end %>
