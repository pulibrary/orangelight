<% online, physical = holding_request_block(document) %>
<h2 class="visually-hidden">Availability</h2>
<% unless online.blank? %>
  <div class="availability--online" aria-label="availability">
    <h3 id="available-online"><%= t('blacklight.holdings.online')  %></h3>
    <%= online if online.present? %>
  </div>
<% end %> 

<% unless physical.empty? %>
  <div class="availability--physical">
    <h3><%= t('blacklight.holdings.print') %></h3>
      <%= physical %>
    </table>
  </div>
<% end %>
