<%= render 'previous_next_doc' %>

<%= render_document_heading_partial %>

<div id="sidebar" class="col-sm-3 col-xs-12 <%= render_document_class %>">
  <%= render_document_sidebar_partial %>
</div>

<div id="content" class="col-sm-9 col-xs-12">
  <%= render_document_main_content_partial %>
</div>

<div id="services" class="availability col-sm-12 col-xs-12">
    
</div>


<div id="librarian-classic-views" class="col-sm-12">
  <%= link_to t('blacklight.tools.librarian_view'), librarian_view_catalog_path(@document), {:id => 'librarianLink'} %>
  | <%= link_to t('blacklight.umlaut'), "#{ENV['umlaut_base']}/resolve?#{@document.export_as_openurl_ctx_kev(document_partial_name(@document))}", {:id => 'getitLink' } %>
  | <%= link_to t('blacklight.voyager'), voyager_url(@document.id), {:id => 'librarianLink', :target => "_blank"} %>
  <span class="glyphicon glyphicon-share-alt"></span>
</div>
