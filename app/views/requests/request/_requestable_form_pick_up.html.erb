  <%= render partial: 'requestable_enum', locals: { requestable: requestable } %>
  <%= render partial: 'requestable_status', locals: { requestable: requestable } %>
  <td class='delivery--options' aria-live="polite">
    <%= render partial: 'requestable_delivery_option_pick_up', locals: { requestable: requestable, mfhd: mfhd, default_pick_ups: default_pick_ups, request_context: request_context, selected: true, fill_in: false } %>  
  </td>
</tr>
